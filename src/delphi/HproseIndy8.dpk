{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Hprose for Delphi with Indy8'}
{$IMPLICITBUILD OFF}

requires
  rtl,
  indy;

contains
  HproseIdHttpClient in 'Indy8\HproseIdHttpClient.pas',
  HproseClient in 'HproseClient.pas',
  HproseCommon in 'HproseCommon.pas',
  HproseIO in 'HproseIO.pas';

end.

